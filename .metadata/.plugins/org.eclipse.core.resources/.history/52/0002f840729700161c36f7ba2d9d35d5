package CharacterController;

import java.util.Random;

import game.engine.*;

public class AI extends CharacterController
{
	float speed = 200f;
	
	public void update()
	{
		Random r = new Random();
		int x = r.nextInt(3) - 1;
		int y = r.nextInt(3) - 1;
		super.move(new Vector2(x, y).multiply(Time.deltaTime * speed));
	}
}