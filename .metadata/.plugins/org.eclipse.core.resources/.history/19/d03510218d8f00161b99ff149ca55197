package Snake;

import game.engine.*;

public class SnakeScene extends Scene
{
	public void activate()
	{
		super.activate();
		GameObject.Create(new GameObject("Main Camera", new Component[] { new Camera() }), new Vector2(), 0f);
		GameObject.Create(new GameObject("Snake", new Component[] { new Snake() }), new Vector2(Screen.width / 2, Screen.height / 2), 0f);
	}
}