package game.engine;

import javafx.scene.Group;

public class Scene extends javafx.scene.Scene
{
	private static Transform root = new Transform();
	
	public Scene() 
	{
		super(root, Screen.width, Screen.height, true);
	}
	
	public void addGameObject(Group group)
	{
		root.getChildren().add(group);
	}
}