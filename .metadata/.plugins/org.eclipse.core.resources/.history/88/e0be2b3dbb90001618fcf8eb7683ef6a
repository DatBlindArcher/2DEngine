package game.engine;

public class Vector3 
{
	public double x;
	public double y;
	public double z;
	
	public Vector3(double x, double y, double z)
	{
		this.x = x;
		this.y = y;
		this.z = z;
	}
	
	public static Vector3 zero()
	{
		return new Vector3(0, 0, 0);
	}
	
	public static Vector3 one()
	{
		return new Vector3(1, 1, 1);
	}
}