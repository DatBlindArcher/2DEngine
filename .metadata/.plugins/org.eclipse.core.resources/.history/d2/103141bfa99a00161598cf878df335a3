package character.controller;

import game.engine.*;

public class Projectile extends CircleCollider
{
	public float speed = 100f;
	
	public void update()
	{
		transform.position.add(transform.forward().multiply(speed * Time.deltaTime));
		super.update();
		super.collide();
	}
	
	public void onCollisionStart(Collider col)
	{
		super.onCollisionStart(col);
		GameObject.destroy(gameObject);
	}
}