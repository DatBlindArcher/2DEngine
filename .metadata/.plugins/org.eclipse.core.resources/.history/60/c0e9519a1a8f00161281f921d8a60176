package game.engine;

public class Collider extends Component
{
	public void onCollisionStart(Collider col)
	{
		System.out.println(gameObject.name + "(" + gameObject.transform.position.x + "," + gameObject.transform.position.y 
				+ ") collided with " + col.gameObject.name + "(" + col.gameObject.transform.position.x + "," + col.gameObject.transform.position.y + ")");
	}
	
	public void onCollisionStay(Collider col)
	{
		
	}
	
	public void onCollisionStop(Collider col)
	{
		
	}
}