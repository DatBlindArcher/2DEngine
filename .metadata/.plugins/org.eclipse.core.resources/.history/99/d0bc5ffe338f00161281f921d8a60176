package game.engine;

import java.awt.Shape;
import java.awt.geom.Area;
import java.awt.geom.GeneralPath;;

public class PolygonCollider extends Collider
{
	public void start()
	{
		super.start();
		Shape shape = new GeneralPath();
		area = new Area(shape);
	}
	
	public void update()
	{
		super.update();
		Shape shape = new GeneralPath();
		area = new Area(shape);
	}
}